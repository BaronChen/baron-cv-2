{
  "name": "techtower-site",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "animate.css": "^3.5.2",
    "bootstrap": "^4.0.0-alpha.6",
    "classnames": "^2.2.5",
    "compression": "^1.7.0",
    "cross-env": "^5.0.2",
    "express": "^4.15.3",
    "isomorphic-fetch": "^2.2.1",
    "node-cmd": "^3.0.0",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.0.2",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-addons-css-transition-group": "^15.6.0",
    "react-addons-transition-group": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.1.3",
    "react-icons": "^2.2.5",
    "react-notification-system": "^0.2.14",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "next",
    "react-scripts": "1.0.10",
    "react-scroll": "^1.5.2",
    "react-snapshot": "next",
    "react-transition-group": "^1.1.3",
    "react-waypoint": "^7.0.3",
    "reactstrap": "^4.6.2",
    "redux": "^3.7.1",
    "redux-devtools-extension": "^2.13.2",
    "redux-form": "^7.0.0",
    "redux-polyglot": "^0.5.2",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.1",
    "s3": "^4.4.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/styles/main.scss -o src/styles",
    "watch-css": "npm run build-css && node-sass-chokidar src/styles/main.scss -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build && react-snapshot && node deploy-scripts/rename-snapshot.js",
    "postbuild": "rimraf build/**/*.map",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "cross-env S3_HOSTING_BUCKET=our-tutorial-site node deploy-scripts/sync-to-s3.js"
  },
  "reactSnapshot": {
    "include": [
      "/",
      "zh",
      "en"
    ],
    "exclude": [
    ],
    "snapshotDelay": 500
  }
}
